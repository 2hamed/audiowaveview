<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#000000"
	android:visibility="visible">


	<ImageView
		android:id="@+id/imageView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:layout_marginLeft="-160dp"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"/>

	<ImageView
		android:id="@+id/imageView2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:layout_marginLeft="-120dp"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"
		android:padding="-2dp"/>

	<ImageView
		android:id="@+id/imageView3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:layout_marginLeft="-80dp"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"
		android:padding="-2dp"/>

	<ImageView
		android:id="@+id/imageView4"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:layout_marginLeft="-40dp"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"
		android:padding="-2dp"/>

	<ImageView
		android:id="@+id/imageView5"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"
		android:padding="-2dp"/>

	<ImageView
		android:id="@+id/imageView6"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:layout_marginLeft="40dp"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"
		android:padding="-2dp"/>

	<ImageView
		android:id="@+id/imageView7"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:layout_marginLeft="80dp"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"
		android:padding="-2dp"/>

	<ImageView
		android:id="@+id/imageView8"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:layout_marginLeft="120dp"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"
		android:padding="-2dp"/>

	<ImageView
		android:id="@+id/imageView9"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:layout_marginLeft="160dp"
		android:focusableInTouchMode="false"
		android:maxHeight="200dp"
		android:maxWidth="42dp"
		android:minHeight="200dp"
		android:minWidth="42dp"
		android:padding="-2dp"/>

	<ImageView
		android:id="@+id/pulse"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:background="#000"
		android:minHeight="200dp"/>

	<ImageView
		android:id="@+id/mask1"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:background="#000"
		android:minHeight="200dp"
		android:visibility="visible"/>

	<ImageView
		android:id="@+id/mask2"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|top"
		android:background="#000"
		android:minHeight="200dp"
		android:visibility="visible"/>

	<SeekBar
		android:id="@+id/Gain"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:layout_marginLeft="0dp"
		android:layout_marginTop="110dp"
		android:max="9"
		android:minWidth="100dp"/>

	<TextView
		android:id="@+id/gain"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:layout_marginRight="100dp"
		android:layout_marginTop="80dp"
		android:text="Gain"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:textColor="#ffffff"/>

	<ImageView
		android:id="@+id/imageView19"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:layout_marginTop="-180dp"
		android:maxWidth="360dp"
		android:minWidth="360dp"
		android:visibility="gone"/>

	<TextView
		android:id="@+id/gainNum"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:layout_marginLeft="100dp"
		android:layout_marginTop="80dp"
		android:text="0 dB"
		android:textAppearance="?android:attr/textAppearanceSmall"
		android:textColor="#ffffff"/>

	<ImageView
		android:id="@+id/imageView14"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"/>

	<CheckBox
		android:id="@+id/fromfile"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="left|bottom"
		android:layout_marginBottom="90dp"
		android:layout_marginLeft="30dp"
		android:text="From file"
		android:textColor="#fff"/>

	<Button
		android:id="@+id/button"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|bottom"
		android:layout_marginBottom="30dp"
		android:singleLine="false"
		android:text="start"/>

	<Button
		android:id="@+id/eq"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|bottom"
		android:layout_marginBottom="30dp"
		android:layout_marginRight="85dp"
		android:text="start"
		android:visibility="visible"/>

	<Button
		android:id="@+id/browse"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:layout_marginTop="110dp"
		android:text="Browse"
		android:visibility="invisible"/>

	<TextView
		android:id="@+id/fileLabel"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="left|center_vertical"
		android:layout_marginLeft="30dp"
		android:layout_marginTop="20dp"
		android:text="File sample rate:"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:textColor="#fff"
		android:visibility="invisible"/>

	<Spinner
		android:id="@+id/sample"
		style="simple_spinner_dropdown_item"
		android:layout_width="147dp"
		android:layout_height="wrap_content"
		android:layout_gravity="right|center_vertical"
		android:layout_marginRight="50dp"
		android:layout_marginTop="20dp"
		android:background="#fff"
		android:entries="@array/sample_rate"
		android:prompt="@string/sample"
		android:visibility="invisible"/>

	<Button
		android:id="@+id/Rec"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|bottom"
		android:layout_marginBottom="30dp"
		android:layout_marginLeft="85dp"
		android:singleLine="false"
		android:text="rcorder"
		android:visibility="gone"/>

	<LinearLayout
		android:id="@+id/equalizer"
		android:layout_width="288dp"
		android:layout_height="389dp"
		android:layout_gravity="left|bottom"
		android:layout_marginBottom="20dp"
		android:layout_marginLeft="20dp"
		android:alpha="1"
		android:background="#111111"
		android:orientation="vertical"
		android:visibility="gone">


	</LinearLayout>


</FrameLayout>